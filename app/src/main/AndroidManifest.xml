<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.martinarroyo.wgutermtracker">

    <application
        android:name=".notification.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TermMainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="logic.AddEditTerm"
            ></activity>
        <activity android:name=".TermDetailActivity"></activity>
        <activity android:name="logic.AddEditCourse"></activity>
        <activity android:name=".CourseDetailActivity"></activity>
        <activity android:name=".NoteDetailActivity"></activity>
        <activity android:name="logic.AddEditAssessment"></activity>
        <activity android:name="logic.AddEditCourseNote"></activity>
        <activity android:name=".AssessmentDetailActivity"></activity>
        <activity android:name="logic.AddEditNotification"></activity>
        <receiver android:name=".notification.AppBroadcastReceiver"></receiver>
    </application>

</manifest>